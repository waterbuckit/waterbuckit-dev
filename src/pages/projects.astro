---
import Link from "../components/Link.astro";
import Main from "../layouts/main.astro";
import { Pages, pageMap } from "../pages";
---

<Main active={pageMap[Pages.PROJECTS].title}>
  <div class="flex flex-col items-center mt-10 w-full">
    <h1 class="text-6xl font-bold">Coming Soon...</h1>
    <p class="text-4xl text-slate-300 mt-10">
      In the meantime, check out my <Link href="https://github.com/waterbuckit">
        GitHub profile
      </Link>!
    </p>
  </div>
</Main>
